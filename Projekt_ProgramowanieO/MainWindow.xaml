<Window x:Class="Projekt_ProgramowanieO.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projekt_ProgramowanieO"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Frame x:Name="MainFrame"/>
        <StackPanel VerticalAlignment="Top" HorizontalAlignment="Right" Orientation="Horizontal" Margin="0 10 0 0">
            <Button 
                Content="Moje rezerwacje" 
                HorizontalAlignment="Left"
                Margin="0 0 20 0"  
                VerticalAlignment="Top" 
                Click="MyReservationsBtn_1"/>
            <Button 
                Content="Panel Admina" 
                HorizontalAlignment="Left" 
                Margin="0 0 20 0"  
                VerticalAlignment="Top" 
                Click="MyDataBtn_1"/>
            <Button 
                Content="Wyloguj" 
                HorizontalAlignment="Left" 
                Margin="0 0 0 0" 
                VerticalAlignment="Top" 
                Click="LogOutBtn_1"/>
        </StackPanel>
        <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Height="260" Margin="48,109,0,0" VerticalAlignment="Top" Width="722" d:ItemsSource="{d:SampleData ItemCount=5}"/>
    </Grid>
</Window>
