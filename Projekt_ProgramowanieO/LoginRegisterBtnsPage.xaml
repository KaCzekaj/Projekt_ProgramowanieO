<Page x:Class="Projekt_ProgramowanieO.LoginRegisterBtnsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Projekt_ProgramowanieO"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="LoginRegisterBtnsPage">

    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Orientation="Horizontal" >
            <Button x:Name="LoginBtn" Content="Login" HorizontalAlignment="Center"  VerticalAlignment="Center" Height="45" Width="86" Margin="0 0 120 0" Click="LoginBtn_1"/>
            <Button x:Name="RegisterBtn" Content="Register" HorizontalAlignment="Center"  VerticalAlignment="Center" Height="45" Width="86"/>
        </StackPanel>
        
    </Grid>
</Page>
